#. module: durpro_payment
#: model:mail.template,body_html:durpro_payment.email_template_payment_notice
msgid ""
"<div style=\"margin: 0px; padding: 0px\">\n"
"                    <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"                        Dear\n"
"                        <t t-if=\"object.partner_id.parent_id\">\n"
"                            <t t-out=\"object.partner_id.name or ''\">Brandon Freeman</t> (<t t-out=\"object.partner_id.parent_id.name or ''\">Azure Interior</t>),\n"
"                        </t>\n"
"                        <t t-else=\"\">\n"
"                            <t t-out=\"object.partner_id.name or ''\">Brandon Freeman</t>,\n"
"                        </t>\n"
"                        Please see the attached payment receipt for our payment of\n"
"                        <t t-out=\"object.amount\" t-options=\"{'widget': 'monetary', 'display_currency': object.currency_id}\">$123.45 CAD</t>\n"
"                        made on <t t-out=\"object.date\" t-options=\"{'widget': 'date'}\">2023-01-01</t>.\n"
"                    </p>\n"
"\n"
"                    <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"                        <br/>Best regards,\n"
"                    </p>\n"
"                    <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"                        <br/>The <t t-out=\"object.company_id.name\"/> accounts payable team.<br/>\n"
"                        <t t-esc=\"object.company_id.ap_email\"/><br/>\n"
"                        <t t-esc=\"object.company_id.partner_id.phone\"/>\n"
"                    </p>\n"
"                </div>\n"
"            "
msgstr "<div style=\"margin: 0px; padding: 0px\">\n"
"                    <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"                        Bonjour \n"
"                        <t t-if=\"object.partner_id.parent_id\">\n"
"                            <t t-out=\"object.partner_id.name or ''\">Brandon Freeman</t> (<t t-out=\"object.partner_id.parent_id.name or ''\">Azure Interior</t>),\n"
"                        </t>\n"
"                        <t t-else=\"\">\n"
"                            <t t-out=\"object.partner_id.name or ''\">Brandon Freeman</t>,\n"
"                        </t>\n"
"                        Veuillez trouver en pièce jointe un avis de paiement pour \n"
"                        <t t-out=\"object.amount\" t-options=\"{'widget': 'monetary', 'display_currency': object.currency_id}\">$123.45 CAD</t>\n"
"                        envoyé le <t t-out=\"object.date\" t-options=\"{'widget': 'date'}\">2023-01-01</t>.\n"
"                    </p>\n"
"\n"
"                    <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"                        <br/>Cordialement,\n"
"                    </p>\n"
"                    <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"                        <br/>L'équipe des comptes à payer <t t-out=\"object.company_id.name\"/><br/>\n"
"                        <t t-esc=\"object.company_id.ap_email\"/><br/>\n"
"                        <t t-esc=\"object.company_id.partner_id.phone\"/>\n"
"                    </p>\n"
"                </div>\n"
"            "

