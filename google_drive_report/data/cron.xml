<?xml version="1.0" encoding="UTF-8"?>
<odoo noupdate="1">
    <record forcecreate="True" id="ir_cron_upload_google_drive_reports" model="ir.cron">
        <field name="name">Upload Reports to Google Drive</field>
        <field name="active" eval="True"/>
        <field name="user_id" ref="base.user_root"/>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="nextcall" eval="(DateTime.now() + relativedelta(days=+1)).strftime('%Y-%m-%d 06:00:00')"/>
        <field name="doall" eval="True"/>
        <field name="model_id" ref="model_google_drive_report_config"/>
        <field name="state">code</field>
        <field name="code">model.search([]).upload_report()</field>
    </record>
</odoo>
